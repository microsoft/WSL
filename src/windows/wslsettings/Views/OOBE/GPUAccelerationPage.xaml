<Page
    x:Class="WslSettings.Views.OOBE.GPUAccelerationPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behaviors="using:WslSettings.Behaviors"
    xmlns:controls="using:WslSettings.Controls"
    behaviors:NavigationViewHeaderBehavior.HeaderMode="Never">

    <controls:OOBEContent x:Uid="Settings_OOBEGPUAcceleration" HeroImage="ms-appx:///Assets/SettingsOOBEGPUAcceleration.gif">
        <controls:OOBEContent.PageContent>
            <StackPanel Orientation="Vertical" Spacing="12">
                <HyperlinkButton x:Uid="Settings_OOBEGPUAccelerationLink" Style="{StaticResource TextButtonStyle}"/>
            </StackPanel>
        </controls:OOBEContent.PageContent>
    </controls:OOBEContent>
</Page>
